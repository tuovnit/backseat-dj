{
  "expo": {
    "name": "Backseat-DJ-App",
    "slug": "Backseat-DJ-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/BDJ-app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/BDJ-logo-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#282B30"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#282B30",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "spotify"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
	  [
		"expo-build-properties",
		{
			"android": {
			"compileSdkVersion": 31,
			"targetSdkVersion": 31,
			"buildToolsVersion": "31.0.0"
			},
			"ios": {
			"deploymentTarget": "15.0"
			}
		}
	  ]
    ],
    "scheme": "Backseat-DJ",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
